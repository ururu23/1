    ## Classify files as None to exclude them from the built distributions.
    build.directory_name = "MirrorMine-0.11"
    build.executable_name = "MirrorMine"

    build.classify('**~', None)
    build.classify('**.bak', None)
    build.classify('**/.**', None)
    build.classify('**/#**', None)
    build.classify('**/thumbs.db', None)

